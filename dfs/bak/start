          /dfs/bin/dfssvr -server-name=`/usr/bin/uuid` \
           --listen-addr=:10000 \
           --zk-addr=dfs-zk \
           --shard-dburi=dfs-shard \
           ---shard-name=shard \
           --event-dbname=event \
           --slog-dbname=slog \
           --metrics-address=:1920 \
           --gluster-log-dir=/tmp/dfssvr \
           --v=2 \
           --logtostderr
